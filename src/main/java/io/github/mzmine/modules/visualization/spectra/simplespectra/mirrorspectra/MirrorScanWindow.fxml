<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="650.0" prefWidth="800.0"
  xmlns="http://javafx.com/javafx/15.0.1"
  fx:controller="io.github.mzmine.modules.visualization.spectra.simplespectra.mirrorspectra.MirrorScanWindowController">
   <center>
      <GridPane BorderPane.alignment="CENTER">
         <children>
            <SplitPane dividerPositions="0.7">
               <items>
                  <BorderPane fx:id="pnMirror">
                     <top>
                        <VBox BorderPane.alignment="CENTER">
                           <children>
                              <Label text="MS/MS mirror (modified cosine)">
                                 <font>
                                    <Font name="System Bold" size="12.0"/>
                                 </font>
                              </Label>
                              <Label text="scores" fx:id="lbMirrorStats"/>
                              <Label text="scores" fx:id="lbMirrorModifiedStats"/>
                           </children>
                           <padding>
                              <Insets left="5.0"/>
                           </padding>
                        </VBox>
                     </top>
                  </BorderPane>
                  <BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                     <center>
                        <ScrollPane fitToHeight="true" fitToWidth="true">
                           <BorderPane>
                              <center>
                                 <TableView BorderPane.alignment="CENTER" fx:id="tableMirror">
                                    <columns>
                                       <TableColumn editable="false" prefWidth="75.0" text="top">
                                          <columns>
                                             <TableColumn prefWidth="75.0" text="mz"
                                               fx:id="colMzTop"/>
                                             <TableColumn editable="false" prefWidth="70.0"
                                               text="intensity" fx:id="colIntensityTop"/>
                                          </columns>
                                       </TableColumn>
                                       <TableColumn editable="false" prefWidth="75.0" text="bottom">
                                          <columns>
                                             <TableColumn editable="false" prefWidth="75.0"
                                               text="mz" fx:id="colMzBottom"/>
                                             <TableColumn editable="false" prefWidth="70.0"
                                               text="intensity" fx:id="colIntensityBottom"/>
                                          </columns>
                                       </TableColumn>
                                       <TableColumn editable="false" prefWidth="25" text="c"
                                         fx:id="colMatchColor"/>
                                       <TableColumn editable="false" prefWidth="75.0" text="match"
                                         fx:id="colMatch"/>
                                       <TableColumn editable="false" prefWidth="100.0"
                                         text="contribution" fx:id="colContribution"/>
                                    </columns>
                                 </TableView>
                              </center>
                           </BorderPane>
                        </ScrollPane>
                     </center>
                  </BorderPane>
               </items>
            </SplitPane>
            <SplitPane GridPane.rowIndex="1" dividerPositions="0.7">
               <items>
                  <BorderPane fx:id="pnNLMirror">
                     <top>
                        <VBox BorderPane.alignment="CENTER">
                           <children>
                              <Label text="Neutral loss mirror">
                                 <font>
                                    <Font name="System Bold" size="12.0"/>
                                 </font>
                              </Label>
                              <Label text="scores" fx:id="lbNeutralLossStats"/>
                           </children>
                           <padding>
                              <Insets left="5.0"/>
                           </padding>
                        </VBox>
                     </top>
                  </BorderPane>
                  <BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                     <center>
                        <ScrollPane fitToHeight="true" fitToWidth="true">
                           <BorderPane>
                              <center>
                                 <TableView BorderPane.alignment="CENTER" fx:id="tableNLMIrror">
                                    <columns>
                                       <TableColumn editable="false" prefWidth="75.0" text="top">
                                          <columns>
                                             <TableColumn prefWidth="75.0" text="mz"
                                               fx:id="colMzTop1"/>
                                             <TableColumn editable="false" prefWidth="70.0"
                                               text="intensity" fx:id="colIntensityTop1"/>
                                          </columns>
                                       </TableColumn>
                                       <TableColumn editable="false" prefWidth="75.0" text="bottom">
                                          <columns>
                                             <TableColumn editable="false" prefWidth="75.0"
                                               text="mz" fx:id="colMzBottom1"/>
                                             <TableColumn editable="false" prefWidth="70.0"
                                               text="intensity" fx:id="colIntensityBottom1"/>
                                          </columns>
                                       </TableColumn>
                                       <TableColumn editable="false" prefWidth="25" text="c"
                                         fx:id="colMatchColor1"/>
                                       <TableColumn editable="false" prefWidth="75.0" text="match"
                                         fx:id="colMatch1"/>
                                       <TableColumn editable="false" prefWidth="100.0"
                                         text="contribution" fx:id="colContribution1"/>
                                    </columns>
                                 </TableView>
                              </center>
                           </BorderPane>
                        </ScrollPane>
                     </center>
                  </BorderPane>
               </items>
            </SplitPane>
         </children>
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES"/>
         </columnConstraints>
         <rowConstraints>
            <RowConstraints vgrow="SOMETIMES"/>
            <RowConstraints vgrow="SOMETIMES"/>
         </rowConstraints>
      </GridPane>
   </center>
   <top>
      <Accordion BorderPane.alignment="CENTER">
         <panes>
            <TitledPane animated="false" graphicTextGap="0.0" text="Spectra selection">
               <content>
                  <GridPane hgap="4.0" vgap="3.0">
                     <children>
                        <HBox GridPane.columnIndex="1" GridPane.rowIndex="3" spacing="5.0">
                           <children>
                              <Button mnemonicParsing="false" onAction="#openGnpsLibExample"
                                text="GNPS library"/>
                              <Button mnemonicParsing="false" onAction="#openUSIExample1"
                                text="USI1"/>
                              <Button mnemonicParsing="false" onAction="#openUSIExample2"
                                text="USI2"/>
                           </children>
                        </HBox>
                        <Label GridPane.halignment="RIGHT" GridPane.rowIndex="1" text="top"/>
                        <Label GridPane.halignment="RIGHT" GridPane.rowIndex="2" text="bottom"/>
                        <Label GridPane.columnIndex="1"
                          text="Spectrum number | GNPS library ID | USI">
                           <font>
                              <Font name="System Bold" size="12.0"/>
                           </font>
                        </Label>
                        <Label GridPane.rowIndex="3" text="examples"/>
                        <TextField GridPane.columnIndex="1" GridPane.rowIndex="1" fx:id="txtTop"/>
                        <TextField GridPane.columnIndex="1" GridPane.rowIndex="2"
                          fx:id="txtBottom"/>
                     </children>
                     <columnConstraints>
                        <ColumnConstraints fillWidth="false" hgrow="NEVER" minWidth="0.0"/>
                        <ColumnConstraints hgrow="ALWAYS"/>
                     </columnConstraints>
                     <rowConstraints>
                        <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
                        <RowConstraints vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
                        <RowConstraints vgrow="SOMETIMES"/>
                     </rowConstraints>
                  </GridPane>
               </content>
            </TitledPane>

            <TitledPane animated="false" graphicTextGap="0.0" text="Parameters" fx:id="pnParams"/>
         </panes>
      </Accordion>
   </top>
</BorderPane>
