<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.github.mzmine.modules.visualization.rawdataoverview.RawDataOverviewWindowController">
   <top>
      <AnchorPane maxHeight="30.0" prefHeight="30.0" prefWidth="800.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="rawDataLabel" layoutX="-4.0" layoutY="6.0" prefHeight="30.0" prefWidth="800.0" style="-fx-font-weight: bold;" text="Overview of raw data file: " AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <padding>
                  <Insets left="5.0" />
               </padding>
            </Label>
         </children>
      </AnchorPane>
   </top>
   <center>
      <SplitPane dividerPositions="0.7" orientation="VERTICAL" BorderPane.alignment="CENTER">
        <items>
            <SplitPane dividerPositions="0.5" prefHeight="568.0" prefWidth="384.0">
               <items>
                  <BorderPane fx:id="chromatogramPane" />
                  <BorderPane fx:id="spectraPane" minWidth="200.0" />
               </items>
            </SplitPane>
            <BorderPane minHeight="150.0" SplitPane.resizableWithParent="false">
               <right>
                  <GridPane fx:id="metaDataGridPane" maxHeight="150.0" minHeight="150.0" minWidth="200.0" prefHeight="150.0" prefWidth="200.0" BorderPane.alignment="TOP_LEFT">
                    <columnConstraints>
                      <ColumnConstraints hgrow="ALWAYS" maxWidth="180.0" minWidth="140.0" prefWidth="140.0">
                              </ColumnConstraints>
                    </columnConstraints>
                    <rowConstraints>
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                     <children>
                        <Label style="-fx-font-weight: bold;" text="Total number of scans:">
                           <GridPane.margin>
                              <Insets left="5.0" />
                           </GridPane.margin>
                        </Label>
                        <Label style="-fx-font-weight: bold;" text="Retention time range:" GridPane.rowIndex="2">
                           <GridPane.margin>
                              <Insets left="5.0" />
                           </GridPane.margin>
                        </Label>
                       <Label style="-fx-font-weight: bold;" text="m/z range:"
                         GridPane.rowIndex="4">
                         <GridPane.margin>
                           <Insets left="5.0"/>
                         </GridPane.margin>
                       </Label>
                       <Label style="-fx-font-weight: bold;" text="Maximum TIC:"
                         GridPane.rowIndex="6">
                         <GridPane.margin>
                           <Insets left="5.0"/>
                         </GridPane.margin>
                       </Label>
                       <Label fx:id="lblNumScans" text="Label" GridPane.rowIndex="1">
                         <GridPane.margin>
                           <Insets left="5.0"/>
                         </GridPane.margin>
                       </Label>
                       <Label fx:id="lblRtRange" text="Label" GridPane.rowIndex="3">
                         <GridPane.margin>
                           <Insets left="5.0"/>
                         </GridPane.margin>
                       </Label>
                       <Label fx:id="lblMzRange" text="Label" GridPane.rowIndex="5">
                         <GridPane.margin>
                           <Insets left="5.0"/>
                         </GridPane.margin>
                       </Label>
                       <Label fx:id="lblMaxTIC" text="Label" GridPane.rowIndex="7">
                         <GridPane.margin>
                           <Insets left="5.0"/>
                         </GridPane.margin>
                       </Label>
                     </children>
                  </GridPane>
               </right>
               <center>
                  <TableView fx:id="rawDataTableView" BorderPane.alignment="CENTER">
                    <columns>
                      <TableColumn fx:id="scanColumn" editable="false" prefWidth="75.0" text="Scan #" />
                      <TableColumn fx:id="rtColumn" editable="false" prefWidth="75.0" text="Retention time" />
                        <TableColumn fx:id="msLevelColumn" editable="false" prefWidth="60.0" text="MS Level" />
                        <TableColumn fx:id="precursorMzColumn" prefWidth="75.0" text="Precursor m/z" />
                        <TableColumn fx:id="mzRangeColumn" prefWidth="75.0" text="m/z Range" />
                        <TableColumn fx:id="scanTypeColumn" editable="false" prefWidth="65.0" text="Scan Type" />
                        <TableColumn fx:id="polarityColumn" editable="false" prefWidth="60.0" text="Polarity" />
                        <TableColumn fx:id="definitionColumn" minWidth="0.0" prefWidth="65.0" text="Scan definition" />
                        <TableColumn fx:id="mobilityColumn" prefWidth="60.0" text="Mobility" />
                    </columns>
                  </TableView>
               </center>
            </BorderPane>
        </items>
      </SplitPane>
   </center>
</BorderPane>
