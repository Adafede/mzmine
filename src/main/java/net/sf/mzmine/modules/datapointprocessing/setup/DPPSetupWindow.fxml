<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="net.sf.mzmine.modules.datapointprocessing.setup.DPPSetupWindowController">
   <children>
      <BorderPane layoutX="-100.0" layoutY="-100.0" prefHeight="100.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <center>
            <SplitPane dividerPositions="0.5" prefHeight="283.0" prefWidth="315.0" BorderPane.alignment="CENTER">
              <items>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="98.0" prefWidth="47.0">
                     <children>
                        <TreeView fx:id="tvProcessing" layoutX="81.0" layoutY="46.0" prefHeight="401.0" prefWidth="320.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                     </children>
                  </AnchorPane>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
                     <children>
                        <TreeView fx:id="tvAllModules" layoutY="42.0" prefHeight="401.0" prefWidth="320.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                     </children>
                  </AnchorPane>
              </items>
            </SplitPane>
         </center>
         <bottom>
            <HBox alignment="CENTER" spacing="10.0">
               <children>
                  <CheckBox fx:id="cbEnabled" mnemonicParsing="false" text="Enabled" />
                  <Button fx:id="btnApply" mnemonicParsing="false" onAction="#btnApplyClicked" text="Apply &amp; Close" />
                  <Button fx:id="btnAdd" mnemonicParsing="false" onAction="#btnAddClicked" text="Add" />
                  <Button fx:id="btnRemove" mnemonicParsing="false" onAction="#btnRemoveClicked" text="Remove" />
                  <Button fx:id="btnSetParameters" mnemonicParsing="false" onAction="#btnSetParamClicked" text="Set parameters" />
                  <Button fx:id="btnSave" disable="true" mnemonicParsing="false" onAction="#btnSaveClicked" text="Save" />
                  <Button fx:id="btnLoad" disable="true" mnemonicParsing="false" onAction="#btnLoadClicked" text="Load" />
               </children>
            </HBox>
         </bottom>
      </BorderPane>
   </children>
</AnchorPane>
