<?xml version="1.0" encoding="iso-8859-1"?>
<!DOCTYPE report PUBLIC "-//JFreeReport//DTD report definition//EN//simple/version 0.8.5" "http://jfreereport.sourceforge.net/report-085.dtd">
<report name="Peaks" pageformat="A4" orientation="landscape">

  <!-- ************* -->
  <!-- * FUNCTIONS * -->
  <!-- ************* -->
  <functions>
  
    <property-ref name="report.date"/>

    <function name="PageNumber" class="org.jfree.report.function.PageFunction"/>
  
  </functions>


    <!-- =============== -->
    <!-- = PAGE HEADER = -->
    <!-- =============== -->
	<pageheader height="20" fontname="sansserif" fontsize="10" fsbold="true">
		<label x="0" y="0" width="100%" height="20" alignment="center" color="#808080">Peak list generated by MZmine 2</label>
	</pageheader>
	
    <!-- =============== -->
    <!-- = PAGE FOOTER = -->
    <!-- =============== -->
	<pagefooter height="24" fontname="sansserif" fontsize="8" fsbold="true" onfirstpage="true">

	    <number-field fieldname="PageNumber" x="0" y="10" width="100%" height="8" alignment="center" format="Page #0"></number-field>

	</pagefooter>


    <!-- ============= -->
    <!-- =  GROUPS   = -->
    <!-- ============= -->
	<groups>
		<group name="All">
			<groupheader repeat="true">
			</groupheader>
		</group>
	</groups>

    <!-- ============= -->
    <!-- = ITEM BAND = -->
    <!-- ============= -->
	<items fontname="sansserif" fontsize="8">
		
	</items>


</report>


